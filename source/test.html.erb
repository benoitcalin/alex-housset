<% data.art.artworks.each do |artwork| %>

<!-- TEST -->
  <div class="test-background">
    <div class="row test-information">
      <div class="col art-and-title">
        <%= image_tag artwork.image, alt: artwork.name, class: 'photo' %>
        <%= image_tag artwork.title, alt: artwork.name, class: 'titre' %>
      </div>
      <div class="col art-texte">
        <%= image_tag artwork.texte, alt: artwork.name, class: 'texte' %>
      </div>
    </div>

    <div class="row test-soundcloud player-test">
      <div class="col soundcloud">
        <iframe width="100%" height="166" scrolling="no" frameborder="no" allow="autoplay"
        src="https://w.soundcloud.com/player/?url=<%= artwork.soundcloud_url %>&amp;{ auto_play=true }">
        </iframe>
      </div>
    </div>
  </div>

<% end %>
